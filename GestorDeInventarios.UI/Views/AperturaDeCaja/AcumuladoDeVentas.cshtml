@model GestionDeInventarios.Model.AcumuladoDeVentas


@{
    ViewData["Title"] = "AcumuladoDeVentas";
}

<h1>Acumulado De Ventas</h1>

<div>
    <h4>Detalles De acumulados</h4>
    <hr />
    <dl class="row">
        <dt class="col-sm-2">
            Acumulado Sinpe Movil
        </dt>
        <dd class="col-sm-10">
            @Html.DisplayFor(model => model.AcumuladoSinpeMovil)
        </dd>
        <dt class="col-sm-2">
            Acomulado Efectivo
        </dt>
        <dd class="col-sm-10">
            @Html.DisplayFor(model => model.AcomuladoEfectivo)
        </dd>
        <dt class="col-sm-2">
            Acumulado Tarjeta
        </dt>
        <dd class="col-sm-10">
            @Html.DisplayFor(model => model.AcumuladoTarjeta)
        </dd>
    </dl>
</div>
<div>
    <a style="display: inline-block; padding: 10px 20px; background-color: #00b3b3; color: white; text-decoration: none; border-radius: 4px;" asp-action="Index">Regresar al Registro</a>
</div>
